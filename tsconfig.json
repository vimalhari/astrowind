{
  "extends": "./node_modules/astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "astro/jsx-runtime": ["node_modules/astro/jsx-runtime.d.ts"],
      "astro/jsx-dev-runtime": ["node_modules/astro/jsx-runtime.d.ts"],
      "~/*": ["src/*"]
    }
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist/"]
}
